<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <Button android:layout_width="wrap_content" android:layout_height="wrap_content"
                android:id="@+id/start" android:onClick="mOnClick" android:text="Start Mpa Activity"/>
        <Button android:layout_width="wrap_content" android:layout_height="wrap_content"
                android:id="@+id/check" android:onClick="mOnClick" android:text="Check Permission"/>
        <Button android:layout_width="wrap_content" android:layout_height="wrap_content"
                android:id="@+id/test" android:onClick="mOnClick" android:text="TestQuery"/>








        <Button android:layout_width="wrap_content" android:layout_height="wrap_content"
                android:id="@+id/friend" android:onClick="mOnClick" android:text="Start friend"/>

        <Button android:layout_width="wrap_content" android:layout_height="wrap_content"
                android:id="@+id/startApp" android:onClick="mOnClick" android:text="Start app"/>
        <Button android:layout_width="wrap_content" android:layout_height="wrap_content"
                android:id="@+id/profile" android:onClick="mOnClick" android:text="profile"/>

</LinearLayout>